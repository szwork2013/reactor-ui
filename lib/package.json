{
  "name": "reactor-ui",
  "version": "0.0.3",
  "description": "Reactor UI Kit",
  "main": "lib",
  "scripts": {
    "build-npm" : "babel -d lib ./src/js && cp README.md lib/ && cp package.json lib/",
    "watch-dev-tabs": "webpack --config webpack.dev.config.js dev/tab.js dev/out/tab.js --watch",
    "less-compile": "lessc src/css/rui.less > dev/out/rui-styles.css"
