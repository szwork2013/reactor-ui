  "dependencies" : {
    "react": "^0.13.1",
    "classnames": "^1.2.0"
  },

  "devDependencies": {
    "babel": "^4.7.16",
    "babel-core": "^4.7.16",
    "babel-loader": "^4.3.0",
    "chai": "^2.2.0",

    "karma": "^0.12.31",
    "karma-chrome-launcher": "^0.1.7",
    "karma-mocha": "^0.1.10",
    "karma-webpack": "^1.5.0",
    "mocha": "^2.2.1",

    "sinon": "^1.14.1",
    "webpack": "^1.7.3"
  }
}
